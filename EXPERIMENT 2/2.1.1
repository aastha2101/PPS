
a ,b , c = map(int,input().split())

root1=f"{(-b+(b**2-4*a*c)**0.5)/(2*a):.2f}"
root2=f"{(-b-(b**2-4*a*c)**0.5)/(2*a):.2f}"

D=b**2-4*a*c
realpart=f"{-b/(2*a):.2f}"
imaginarypart=f"{(-D)**0.5/(2*a):.2f}"
if(D>0):
	print("root1 =",root1)
	print("root2 =",root2)
elif(D==0):
	print("root1 = root2 =",root1)
else:
	print(f"root1 = {realpart}+{imaginarypart}i")
	print(f"root2 = {realpart}-{imaginarypart}i")
